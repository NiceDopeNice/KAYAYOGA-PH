@import 'fonts'

// color
$lr = #C41967
$lr_hover = #E1247A
$lr_disabled = #AB0551
$lv = #461D84
$black = #000
$light_black = #171717
$white = #fff

*,
*:before,
*:after
    user-select: none
    box-sizing: border-box
    margin: 0
    word-break: break-word !important

input, textarea 
  -webkit-user-select: text
  -khtml-user-select: text
  -moz-user-select: text
  -ms-user-select: text
  user-select: text
  
body
    position: relative
    overflow-x: hidden

a
    text-decoration: none
button.k_btn
    border-radius: 50px
    font_family(17px, false, 'proxima_bold')
    background: $lr
    border: none
    color: $white
    cursor: pointer
    outline: none
    transition:  ease-in-out .3s
    &:hover
        background: $lr_hover
        transition: ease-in-out .3s
        box-shadow: 0px 10px 15px #0033
    &.k_disabled
        background: $lr_disabled
        cursor: auto
        &:hover
            background: $lr_disabled
            box-shadow: none

.viewer-toolbar, .viewer-title
    display: none !important
.viewer-transition
    z-index: 999999 !important
.viewer-backdrop {
    background-color: rgba(0, 0, 0, 0.7) !important;
}

.k_loading_cont
    position: fixed
    top: 0
    bottom: 0
    left: 0
    right: 0
    display: flex
    justify-content: center
    align-items: center
    z-index: 999999
    background: rgba(255,255,255, 1);
    &.k_contact_body
        background: rgba(255,255,255,0.7) !important;
    .k_loading_main
        position: relative
        display: flex
        justify-content: center
        align-items: center
        .k_loading
            border-radius: 50%
            border-top: 4px solid $lr
            box-shadow: inset 0 0 0 2px rgba(196, 25, 103, 0.1)
            width: 200px
            height: 200px
            -webkit-animation: spin 2s linear infinite
            animation: spin 2s linear infinite
            @media screen and (max-width: 1024px)
                width: 150px
                height: 150px
        img
            position: absolute
            width: 100px
            @media screen and (max-width: 1024px)
                width: 90px
@-webkit-keyframes spin {
    0% {
        -webkit-transform rotate(0deg)
        transform rotate(0deg)
    }
    100% {
        -webkit-transform rotate(360deg)
        transform rotate(360deg)
    }
}
@keyframes spin{
    0% {
        -webkit-transform rotate(0deg)
        transform rotate(0deg)
    }
    100% {
        -webkit-transform rotate(360deg)
        transform rotate(360deg)
    }
}

