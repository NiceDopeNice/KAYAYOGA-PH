    .k_contact_cont
        max-width: 1100px
        width: 90%
        margin:0 auto
        margin-top: 100px
        text-align: center
        h1  
            font_family(35px, false, 'proxima_bold')
        .k_contact_main
            display: flex
            justify-content: space-between
            @media screen and (max-width: 1024px)
                flex-direction: column
                max-width: 500px
                width: 90%
                margin: 0 auto
            .k_contact_left
                text-align: left
                flex: 0 0 45%
                .k_contact_fg
                    position relative
                    padding 15px 0 0
                    margin-top 30px
                    width 100%
                    #error
                        color: $lr_hover
                        font_family(15px, false, 'proxima_regular')
                        position: absolute
                        left: 14px
                        bottom: -20px
                    .arrow 
                        border: solid $lv
                        border-width: 0 1px 1px 0
                        display: inline-block
                        padding: 4px
                        &.down 
                            transform: rotate(45deg)
                            -webkit-transform: rotate(45deg)
                            position: absolute
                            bottom: 15px
                            right: 15px
                    .k_contact_field
                        -webkit-appearance: none
                        -moz-appearance:    none
                        appearance:         none
                        font-family inherit
                        width 100%
                        border 0
                        border-bottom 1px solid $black
                        outline 0
                        font_family(21px, false, 'proxima_regular')
                        color $lr
                        padding 7px 0
                        background transparent
                        transition border-color 0.2s
                        padding-left: 15px
                        &::placeholder
                            color transparent
                        &:placeholder-shown
                            & ~ .form__label
                                font_family(16px, false, 'proxima_regular')
                                cursor text
                                top 20px
                                left: 15px
                        &:focus
                            padding-bottom 6px
                            font-weight 300
                            border-width 2px
                            border-image linear-gradient(to right, $lr, $lr_hover)
                            border-image-slice 1
                            & ~ .form__label
                                position absolute
                                top 0
                                display block
                                transition 0.2s
                                font-size 14px
                                color $black
                        &.k_ph_message
                            height: 200px
                            resize: none
                            &::placeholder
                                color #CFCFCF
                                font_family(22px, false, 'proxima_regular')
                                @media screen and (max-width: 500px)
                                    font-size: 18px
                            &:placeholder-shown
                                & ~ .form__label
                                    cursor text
                                    top 0px
                        &.k_select *
                            padding-left: 10px

                    .form__label
                        position absolute
                        top 0
                        display block
                        transition 0.2s
                        font_family(16px, false, 'proxima_regular')
                        color $black
                        left: 15px

                    .k_contact_field:required, .k_contact_field:invalid
                        box-shadow none
                .k_btn_hldr
                    text-align: center
                    margin-top: 40px
                    .k_btn
                        padding: 18px 0px
                        max-width: 300px
                        width: 90%
                        @media screen and (max-width: 500px)
                            width: 100%
                            max-width: 100%
            .k_contact_right
                flex: 0 0 45%
                padding-top: 60px
                @media screen and (max-width: 1200px)
                    flex: 0 0 47%
                .k_contact_item
                    display: flex
                    justify-content: flex-start
                    align-items: center
                    padding-bottom: 30px
                    font_family(20px, false, 'proxima_regular')
                    @media screen and (max-width: 1200px)
                        font-size: 17px
                    @media screen and (max-width: 550px)
                        font-size: 15px
                        text-align: left
                    img
                        margin-right: 20px
                        @media screen and (max-width: 400px)
                            margin-right: 15px
                    &:nth-of-type(3)
                        img
                            margin-left: -7px
